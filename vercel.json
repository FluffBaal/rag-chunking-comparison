{
  "functions": {
    "api/chunking.py": {
      "maxDuration": 60,
      "memory": 1024
    },
    "api/evaluation.py": {
      "maxDuration": 120,
      "memory": 2048
    },
    "api/analysis.py": {
      "maxDuration": 30,
      "memory": 512
    },
    "api/extract_pdf.py": {
      "maxDuration": 30,
      "memory": 512
    },
    "api/advanced_test_generator.py": {
      "maxDuration": 60,
      "memory": 1024
    },
    "api/ragas_metrics.py": {
      "maxDuration": 60,
      "memory": 1024
    },
    "api/test_generator.py": {
      "maxDuration": 60,
      "memory": 1024
    }
  },
  "env": {
    "VERCEL_FORCE_PYTHON_STREAMING": "1"
  },
  "regions": ["iad1"],
  "framework": "nextjs",
  "buildCommand": "npm run build:vercel",
  "outputDirectory": ".next",
  "installCommand": "npm install"
}

